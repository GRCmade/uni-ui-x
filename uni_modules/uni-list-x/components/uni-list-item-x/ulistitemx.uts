export type ExtraIcon = {
	type: string
	color: string
	size: number
}

export type CustomStyle = {
	padding:string
	backgroundColor:string
}

export type Padding = {
	top: string
	right: string
	bottom: string
	left: string
}

export function showValueInVue<T>(obj: any): T {
    let stringObj = JSON.stringify(obj);
    return JSON.parse(stringObj) as T;
}